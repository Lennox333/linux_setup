<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1746931585369512-1</unique-id>
	<command>kitty -d %f</command>
	<description>Example for a custom action</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon></icon>
	<name>Extract here</name>
	<submenu></submenu>
	<unique-id>1747048924337193-1</unique-id>
	<command>kitty -e --title &quot;Kitty Popup&quot; -- sh -c &quot;~/.local/bin/extract %F; read -p &apos;Press Enter to close...&apos;&quot;</command>
	<description></description>
	<range>*</range>
	<patterns>*.7z;*.zip;*.rar;*.tar.gz</patterns>
	<other-files/>
</action>
<action>
	<icon></icon>
	<name>Set as wall</name>
	<submenu></submenu>
	<unique-id>1747662686548660-1</unique-id>
	<command>wall.sh img %f</command>
	<description></description>
	<range>*</range>
	<patterns>*.jpg;*.png;*.jpeg</patterns>
	<image-files/>
</action>
<action>
	<icon></icon>
	<name>Zip files</name>
	<submenu></submenu>
	<unique-id>1748085094398375-1</unique-id>
	<command>7z a -tzip %F</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
